    <file src="..\..\..\bin\Release\ss.min.js" target="content\Scripts" />
    <file src="..\..\..\bin\Release\ssloader.js" target="content\Scripts" />
    <file src="..\..\..\bin\Release\ssloader.min.js" target="content\Scripts" />
  </files>
</package>
