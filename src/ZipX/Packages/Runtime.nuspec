<?xml version="1.0"?>
<package xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>ScriptSharp.Runtime</id>
    <version>$version$</version>
    <title>Script# Runtime and Script Deployment Support</title>
    <authors>Nikhil Kothari</authors>
    <copyright>Copyright (c) 2012, Nikhil Kothari</copyright>
    <projectUrl>http://scriptsharp.com</projectUrl>
    <iconUrl>http://scriptsharp.com/nuget/Package.png</iconUrl>
    <licenseUrl>http://scriptsharp.com/nuget/License.txt</licenseUrl>
    <summary>Provides the core javascript runtime (ss.js) for Script#-based scripts and script deployment functionality into Web application projects.</summary>
    <description>
      This package contains ss.js and related scripts used by script# generated scripts at runtime. It also adds script deployment functionality, to copy generated scripts from referenced script# projects into Web applications.
    </description>
    <tags>script scriptsharp javascript html aspnet mvc thescriptsharp</tags>
    <language>en-US</language>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <file src="..\..\..\bin\Release\ss.js" target="content\Scripts" />
    <file src="..\..\..\bin\Release\ss.min.js" target="content\Scripts" />
    <file src="..\..\..\bin\Release\ssloader.js" target="content\Scripts" />
    <file src="..\..\..\bin\Release\ssloader.min.js" target="content\Scripts" />
    <file src="..\..\..\bin\Release\ScriptSharp.Deployment.dll" target="tools" />
    <file src="..\..\..\bin\Release\ScriptSharpWeb.targets" target="tools" />
    <file src="Runtime\Install.ps1" target="tools" />
    <file src="Runtime\Uninstall.ps1" target="tools" />
    <file src="Runtime\Reset.psm1" target="tools" />
  </files>
</package>